/* pages/user/user.wxss */
page{
  background: #ddd;
  padding: 10px;
}
.wrap{
  padding-bottom: 100rpx;
  overflow: hidden;
}
.title{
  text-align: center;
  font-size: 32rpx;
  line-height: 120rpx;
  color: #999;
}
.list{
  padding:4px;
  background: #fff;
  float: left;
  box-sizing: border-box;
  margin: 1px;
}
.list image{
  width: 100%;
  height: 100%;
}
.footer{
  clear: both;
  text-align: center;
  padding:100rpx 0 0rpx;
}
.footer image{
  width: 240rpx;
  height: 50rpx;
  
}