
  .wrap{
  box-sizing: border-box;
  width: 100%;
  background: #ffffff;
  padding: 10px
  }
  .success{
  position: relative;
  width: 100%;
  height: 180px;
}
.noUser{
  font-size: 32rpx;
  color: #999;
}
.title{
  font-size: 16px;
  color: #000;
  font-weight: 800;
  margin: 10px 0;
  text-align: center;
}
.fail{
  text-align: center;
  margin: 10px 0;
  font-size: 18px;
  color: #FEC100;
  font-weight: 800;
}
.success .failImg{
  position: absolute;
  width: 60%;
  height: 130px;
  left: 20%;
  top: 40px;
  z-index: 90
}
.success .huaTi{
  position: absolute;
  width: 42%;
  font-size: 16px;
  font-weight: 800;
  color: #7600C1;
  bottom: 24px;
  left: 48px;
  z-index: 120;
  text-align: left;
}
.success .sucImg{
  position: absolute;
  width: 90%;
  height: 180px;
  left: 5%;
  top: 10px;
  z-index: 90
}
.success .share{
  position: absolute;
  right:44px;
  bottom: 20px;
  width: 70px;
  height: 70px;
  border-radius: 50%;
  z-index: 99;
}
.share{
  background: none;
  border: none;
}
.redBtn{
  text-align: center;
  margin: 15px 0;
}
.redBtn .btn{
  padding: 10px 40px;
  height: 40px;
  border-radius: 20px;
  background: #E43E16;
  color: #fff;
  line-height: 40px;
}
.redBtn navigator{
  background: transparent;
  display: inline-block;
}
.redBtn .btn:active{
  background: rgb(230, 85, 49);
}
.noTxt{
  font-size: 14px;
  color: #999;
  margin-bottom: 20px;
}
.result{
  text-align: center;
}

.divider{
  padding-top: 15px!important;
  font-size: 18px!important;
}
.resultImg{
  text-align: center;
}
.resultImg image{
  width: 100px;
  height: 100px;
  border-radius: 50%;
}
.more .left{
  float: left;
}
.more .left image{
  float: left;
  width: 48rpx;
  height: 48rpx;
  margin-right: 20rpx;
}
.more .left .txt{
  float: left;
  height:48rpx;
  line-height:48rpx;
}
.red{
  color: #E43E16;
}
.person .txt {
  color: #999999;
  margin-bottom: 10px;
  font-size: 14px;
}
.person .only{
  vertical-align: middle;
  margin-top: 4px;
  font-size: 16px;
  color: #999;
  text-align: right
}
.person .only .icon{
    vertical-align: top;
    margin-top: 1px;
}
.only .onlytxt{
  margin-right: 10px;
}
.resultImg .red{
  font-size: 16px;
  color: #E43E16;
  margin: 10px 0;
}
.resultImg .prize{
  font-size: 16px;
  color: #333;
 
}

.list .listText{
  font-size: 14px;
  width: 100%;
  border-radius: 14rpx;
  overflow: hidden;
  background: #fff;
}
.wrapColor{
  background: #E9EDF1;
  padding:0 20rpx 100rpx;
}
.listHader{
  position: relative;
  text-align: center;
  color: #E43E16;
  padding:40rpx 0 ;
  width: 100%;
}
.listHader image{
  width:350rpx;
  position: absolute;
  height: 60rpx;
  z-index: 12;
  left: 50%;
  top: 26rpx;
  margin-left: -175rpx;
}
.listImg{
  position: relative;
  height: 400rpx;
}

.listImg>image{
  position: absolute;
  z-index: 11;
  top: 0;
  left: 0;
  width: 100%;
  height: 400rpx;
}
.listImg .jp{
  display: inline-block;
  position: absolute;
  bottom: 0;
  height: 54rpx;
  line-height: 54rpx;
  padding: 0 50rpx;
  right: 0;
  z-index: 12;
  background: #E43E16;
  color: #fff;
  border-radius: 14rpx 0 0 0;
  

}
.listBot{
  padding: 0 20rpx 30rpx;
}
.listBot .firstTxt{
  padding: 20rpx 0;
  font-weight: 800;
}
.listBot .firstTxt .first{
  margin-right:10rpx;
}
.listBot .time{
 font-size: 12px;
}
.listBot .left{
  color: #999999;
}
.listBot .red{
  text-align: right;
  color: #E43E16
}
.listBot .divider{
  height:50rpx!important;
}
.more .left{
  float: left;
}
.more .left image{
  float: left;
  width: 25px;
  height: 25px;
  margin-right: 20rpx;
}
.more .left .txt{
  float: left;
  height:24px;
  line-height: 24px;
}
.listBot .cell{
  padding-top: 4rpx!important;
}
.contact{
  width: 240rpx;
  height: 80rpx;
  line-height:80rpx;
  margin: 80rpx auto; 
  background: #fff;
  color: #666666;
  font-size:32rpx;
  text-align: center;
  border-radius: 40rpx;
}
.navigator-hover{
  background: #E9EDF1;
  border-radius: 40rpx;
}
.footer{
  text-align: center;
  padding: 60rpx 0 0rpx;
}
.footer image{
  width: 240rpx;
  height: 50rpx;
}