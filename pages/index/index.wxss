.header{
  height: 100rpx;
  line-height: 100rpx;
  padding: 0;
  margin: 0;
  text-align: center;
  width: 100%;
  background: #f00;
  color: #fff;
  font-size: 14px
}
.return{
  float: right;
  margin-right:20rpx
}
.list .listText{
  font-size: 14px;
  width: 100%;
  border-radius: 14rpx;
  overflow: hidden;
  background: #fff;
}
.content{
  background: #E9EDF1;
  padding:0 20rpx 160rpx;
}
.listHader{
  text-align: center;
  color: #E43E16;
  padding: 24rpx 0 ;
}
.listImg{
  position: relative;
  height: 400rpx;
}

.listImg>image{
  position: absolute;
  z-index: 11;
  top: 0;
  left: 0;
  width: 100%;
  height: 400rpx;
}
.listImg .jp{
  display: inline-block;
  position: absolute;
  bottom: 0;
  height: 54rpx;
  line-height: 54rpx;
  padding: 0 50rpx;
  right: 0;
  z-index: 12;
  background: #E43E16;
  color: #fff;
  border-radius: 14rpx 0 0 0;
  

}
.listBot{
  padding: 0 20rpx 30rpx;
}
.listBot .firstTxt{
  padding: 20rpx 0;
  font-weight: 800;
}
.listBot .firstTxt .first{
  margin-right:10rpx;
}
.listBot .time{
 font-size: 12px;
}
.listBot .left{
  color: #999999;
}
.listBot .red{
  text-align: right;
  color: #E43E16
}
.listBot .divider{
  height:50rpx!important;
}

.listBot .cell{
  padding-top: 4rpx!important;
}





